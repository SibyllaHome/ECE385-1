wave create -driver freeze -pattern counter -startvalue 0000000000000000 -endvalue 1111111111111111 -type Range -direction Up -period 50ns -step 1 -repeat forever -range 15 0 -starttime 0ns -endtime 1000ns sim:/carry_lookahead/Switches
wave create -pattern constant -value 000000000000000 -range 16 0 -starttime 0ns -endtime 1000ns sim:/carry_lookahead/R
add wave -position end  sim:/carry_lookahead/Sum
add wave -position end  sim:/carry_lookahead/Overflow
add wave -position end  sim:/carry_lookahead/Run
wave create -driver freeze -pattern constant -value 1 -starttime 0ps -endtime 1000ps sim:/carry_lookahead/Run
wave modify -driver freeze -pattern constant -value St1 -starttime 0ns -endtime 1000ns Edit:/carry_lookahead/Run
run -all
# ** Warning: (vsim-8315) U:/Carry-Lookahead/control.sv(17): No condition is true in the unique/priority if/case statement.
# 
# ** Warning: (vsim-8315) U:/Carry-Lookahead/control.sv(17): No condition is true in the unique/priority if/case statement.
# 
# A time value could not be extracted from the current line
# A time value could not be extracted from the current line
wave create -driver freeze -pattern clock -initialvalue HiZ -period 50ns -dutycycle 50 -starttime 0ns -endtime 1000ns sim:/carry_lookahead/Clk
wave create -driver freeze -pattern constant -value 0 -starttime 0ps -endtime 1000ps sim:/carry_lookahead/Reset
wave modify -driver freeze -pattern constant -value St0 -starttime 0ns -endtime 50ns Edit:/carry_lookahead/Reset
wave modify -driver freeze -pattern constant -value 0 -starttime 0ns -endtime 1000ns Edit:/carry_lookahead/Run
run -all
# ** Warning: (vsim-8315) U:/Carry-Lookahead/control.sv(17): No condition is true in the unique/priority if/case statement.
